<page paging="true" nocache="false" rifoptimize="false" output-encoding="UTF-8">
    <external file="forms.css" type="css"/>
    <container>
        <column>
            <FORM id="pollform" action="PollResultsPage.rbml" method="POST">
                <styles>
                    <style name="color" value="#000000"/>
                </styles>
                <text id="pollerror"></text>
                <row class="pollTD">
                    <cell align="inline" width="10%">
                        <input type="radio" name="nb_pollAnswer" value="1"></input>
                    </cell>
                    <cell align="inline">
                        <text>Team Sara</text>
                    </cell>
                </row>
                <row class="pollTD">
                    <cell align="inline" width="10%">
                        <input type="radio" name="nb_pollAnswer" value="2"></input>
                    </cell>
                    <cell align="inline">
                        <text>Team Grissom</text>
                    </cell>
                </row>
                <input type="hidden" name="nb_subPollId" value="Feb25_2013_CSITeams"></input>
                <input type="hidden" name="nb_formSourcePage" value="http://m.cbs.com/index.rbml"></input>
                <row>
                    <cell align="center">
                        <input type="submit" value="Submit"></input>
                    </cell>
                </row>
            </FORM>
        </column>
    </container>
</page>